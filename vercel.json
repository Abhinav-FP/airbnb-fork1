{
  "version": 2,
  "builds": [
    { "src": "next.config.js", "use": "@vercel/next" }
  ],
  "rewrites": [
    { "source": "/", "destination": "/" }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/404",
      "dest": "/404.jsx"
    },
    {
      "src": "/",
      "dest": "/"
    }
  ]
}
